{
 "name": "@pengubot/bot",
 "version": "3.0.0",
 "description": "PenguBot is a multi-purpose discord bot",
 "main": "dist/index.js",
 "repository": "https://github.com/PenguBot/bot.git",
 "author": "Aditya N. Tripathi <adityatripathidelhi@gmail.com>",
 "license": "Apache-2.0",
 "private": true,
 "scripts": {
  "lint": "eslint --fix --ext ts src",
  "test": "eslint --ext ts src",
  "build": "tsc -p .",
  "watch": "tsc -p . -w",
  "clean": "rm -rf dist; yarn build"
 },
 "dependencies": {
  "axoncore": "github:khaaz/AxonCore#dev",
  "chalk": "^2.4.2",
  "discord.js": "^12.0.1",
  "module-alias": "^2.2.2"
 },
 "devDependencies": {
  "@commitlint/cli": "^9.1.1",
  "@commitlint/config-conventional": "^9.1.1",
  "@pengubot/eslint-config": "^1.0.0",
  "@types/jest": "^26.0.7",
  "@types/module-alias": "^2.0.0",
  "@types/node": "^14.0.27",
  "@typescript-eslint/eslint-plugin": "^3.7.1",
  "@typescript-eslint/parser": "^3.7.1",
  "cz-conventional-changelog": "^3.2.0",
  "eclint": "^2.8.1",
  "eslint": "^7.5.0",
  "husky": "^4.2.5",
  "jest": "^26.1.0",
  "jest-circus": "^26.1.0",
  "lint-staged": "^10.2.11",
  "standard-version": "^8.0.2",
  "ts-jest": "^26.1.4",
  "typescript": "^3.9.7"
 },
 "husky": {
  "hooks": {
   "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
   "pre-commit": "yarn lint-staged"
  }
 },
 "commitlint": {
  "extends": [
   "@commitlint/config-conventional"
  ]
 },
 "lint-staged": {
  "*.ts": [
   "eslint --fix --ext ts",
   "eclint fix src"
  ]
 },
 "config": {
  "commitizen": {
   "path": "./node_modules/cz-conventional-changelog"
  }
 },
 "publishConfig": {
  "access": "public"
 },
 "_moduleAliases": {
  "@utils": "dist/lib/util",
  "@lib": "dist/lib",
  "@root": "dist"
 }
}
